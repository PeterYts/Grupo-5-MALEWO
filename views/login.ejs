<%- include('./partials/head', { title:'Login', hojaCss:'styles-login'}) %>
<%- include('./partials/header') %>
    <main>
        <section class="main-title">
            <h3 class="create-account">Iniciar sesión</h3>
            <div class="style-bar1"></div>
        </section>
        <section class="form-section">
            <form class="form">
                <label for="email" class="form-item">Email</label>
                <input type="email" id="email" name="email" class="form-item">
                <label for="password" class="form-item">Contraseña</label>
                <input type="password" id="password" name="password" class="form-item">
                <a href="/retrieve-Acount" class="form-item">¿Has olvidado tu contraseña?</a>
                <a href="/register" class="form-item">Registrarse</a>
                <button type="submit" class="form-item">Iniciar Sesión</button>
            </form>
        </section>
    </main>
<%- include('./partials/footer') %> 
    <script src="../js/prueba.js"></script>
</body>
</html>