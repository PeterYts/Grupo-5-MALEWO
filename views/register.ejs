<%- include('./partials/head', { title:'Register', hojaCss:'styles-register'}) %>
<%- include('./partials/header') %>
    <main>
        <section class="main-title">
            <h3 class="create-account">creá tu cuenta</h3>
            <div class="style-bar1"></div>
        </section>
        <section class="form-section">
            <form action="/register/" method="POST" enctype="multipart/form-data" class="form">
                <label for="name">Nobre de Usuario</label>
                <input type="text" id="name" name="user-name">
                <label for="email">Email</label>
                <input type="email" id="email" name="email">
                <label for="Image">Imagen de Perfil</label>
                <input type="file" name="Image" id="Image" accept=".jpg, .png, .jpeg, .tif, .tiff" class="file-input">
                <label for="tel">Número de Teléfono</label>
                <input type="tel" id="tel" name="num-tel">
                <label for="password">Contraseña</label>
                <input type="password" id="password" name="user-password">
                <button type="submit">Crear Cuenta</button>
            </form>
        </section>
    </main>
<%- include('./partials/footer') %> 
    <script src="../js/prueba.js"></script>
</body>

</html>